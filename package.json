{
  "name": "groverbot2",
  "version": "1.0.0",
  "description": "attempting chatbot again",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
